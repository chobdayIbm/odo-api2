{
  "name": "express-starter",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint './src/**/*.{ts,js}'",
    "lint-fix": "eslint './src/**/*.ts' --fix",
    "start": "node ./dist/index.js",
    "start-dev": "NODE_ENV=development ts-node-dev src/index.ts",
    "test": "NODE_ENV=development jest --forceExit --runInBand --detectOpenHandles",
    "test-watch": "NODE_ENV=development jest  --runInBand"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.9.1",
    "@types/mongoose": "^5.5.43",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.9",
    "tslib": "^1.10.0",
    "yarn": "^1.21.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/mongodb-memory-server": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "mongodb-memory-server": "^5.2.11",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": "12.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "testURL": "",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
